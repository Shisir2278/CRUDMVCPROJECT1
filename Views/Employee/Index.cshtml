@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<a asp-action="Create">Create</a>
<table>
    <tr>
        <td>Id</td>
        <td>Name</td>
        <td>Salary</td>
    </tr>
    @foreach (var d in Model)
    {
        <tr>
            <td>@d.Id</td>
            <td>@d.Name</td>
            <td>@d.Salary</td>
            <td>

                <a asp-action="Delete" asp-route-id="@d.Id">Delete</a>
            </td>

            <a asp-action="Edit" asp-route-id="@d.Id">Edit</a>

        </tr>

    }
</table>